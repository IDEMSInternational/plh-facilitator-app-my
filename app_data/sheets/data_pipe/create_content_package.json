{
  "flow_type": "data_pipe",
  "flow_name": "create_content_package",
  "status": "released",
  "rows": [
    {
      "operation": "filter",
      "input_source": "tag_data",
      "args_list": [
        "type == \"package\""
      ]
    },
    {
      "operation": "append_columns",
      "output_target": "package_tag",
      "args_list": [
        "name: @row.id",
        "text: @row.title"
      ]
    }
  ],
  "_xlsxPath": "data/content_data_pipes.xlsx"
}